{
  "exclude": [
    "venv/**",
    "backend/logs/**",
    "tests/mini_crawl_output/**",
    "frontend/node_modules/**",
    "scripts/node_modules/**",
    "**/__pycache__/**",
    "**/*.pyc",
    ".git/**",
    ".langgraph_api/**",
    "frontend/tsconfig.tsbuildinfo",
    "frontend/.next/**",
    "frontend/out/**",
    "tests/test_*.py",
    "*.tar.gz",
    "*.json",
    "*.csv",
    "*.txt"
  ],
  "include": [
    "backend/**/*.py",
    "tests/__init__.py"
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.13",
  "venvPath": ".",
  "venv": "venv",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonPath": "venv/bin/python",
      "extraPaths": ["."]
    }
  ]
} 